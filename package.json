{
  "name": "Nezaboodka IDE",
  "version": "0.1.0",
  "description": "IDE for artel",
  "license": "MIT",
  "main": "index.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/xelAraY/Nezaboodka_IDE"
  },
  "scripts": {
    "lint:fix": "eslint --fix \"source/**/*.ts\" \"library/**/*.ts\"",
    "start": "vite",
    "start:prod": "npm run pack && vite preview",
    "build": "tsc && eslint \"source/**/*.ts\" \"library/**/*.ts\"",
    "pack": "vite build",
    "install-all": "npm i && npm i --prefix library/reactronic && npm i --prefix library/verstak && npm i --prefix library/verstak-markdown"
  },
  "dependencies": {
    "markdown-it": "13.0.1",
    "markdown-it-prism": "2.3.0",
    "medium-editor": "5.23.3",
    "monaco-editor": "0.34.1",
    "normalize-url": "8.0.0",
    "prismjs": "1.29.0"
  },
  "devDependencies": {
    "@emotion/css": "11.10.5",
    "@types/jsdom": "20.0.0",
    "@types/medium-editor": "5.0.5",
    "@types/node-fetch": "3.0.3",
    "@types/prismjs": "1.26.0",
    "@types/yargs": "17.0.13",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "ava": "4.3.1",
    "eslint": "8.27.0",
    "jsdom": "20.0.2",
    "node-fetch": "3.2.10",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "vite": "3.2.2",
    "vite-tsconfig-paths": "3.5.2",
    "yargs": "17.6.2"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
